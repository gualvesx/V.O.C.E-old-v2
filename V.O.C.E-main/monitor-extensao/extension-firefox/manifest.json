{
  "manifest_version": 2,
  "name": "V.O.C.E",
  "version": "1.1",
  "description": "Monitora o tempo de uso de sites e envia para an√°lise. Icons by Icons8",

  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "nativeMessaging",

    "http://10.111.9.63:8081/*",
    "http://localhost:8081/*",
    "http://localhost:3000/*"
  ],

  "background": {
    "scripts": ["js/background.js"]
