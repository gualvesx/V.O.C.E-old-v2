{
  "manifest_version": 3,
  "name": "V.O.C.E",
  "version": "1.1",
  "description": "Monitora o tempo de uso de sites e envia para an√°lise. Icons by Icons8",
  "minimum_chrome_version": "114",

  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "nativeMessaging"
  ],

  "host_permissions": [
    "http://10.111.9.63:8081/*",
    "http://localhost:8081/*",
    "http://localhost:3000/*"
  ],

  "background": {
    "service_worker": "js/background.js"
  },

  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": true
  },

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },

  "action": {
    "default_popup": "html/options.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  }
}
